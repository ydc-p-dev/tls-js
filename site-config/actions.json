    "modaoperandi.com": {
      "baseUrl": "https://www.modaoperandi.com",
      "productUrl": "https://www.modaoperandi.com/women/products/bags",
      "type": "playwright",
      "actions": [
        { "name": "goToProduct", "selectors": ["a.VariantCell__image"], "type": "click", "waitAfter": 5000, "required" : true, "event" : "[🛒] Go to first product page..." },
        { "name": "cookies", "selectors": ["#_evidon-accept-button"], "type": "click", "waitAfter": 5000, "required" : false, "event" : "[🛒] Closening cookies..." },
        { "name": "popupModal", "selectors": ["button[aria-label=\"Close\"].EmailSignup__close"], "type": "click", "waitAfter": 5000, "required" : false, "event" : "[🛒] Closing popup modal..." },
        { "name": "addToCart", "selectors": ["button.AddToBagButton"], "type": "click", "waitAfter": 5000, "required" : true, "event" : "[🛒] Adding product to cart..." },
        { "name": "viewCart", "selectors": [], "type": "goto", "url":"https://www.modaoperandi.com/shopping-bag", "waitAfter": 5000, "required" : true, "event" : "[🛍️] Viewing cart..." },
        { "name": "openCouponInput", "selectors": ["button[aria-label=\"Add Gift Code\"]"], "type": "click", "waitAfter": 5000, "required" : true, "event" : "[💳] Open coupon input..."  },
        { "name": "enterCoupon", "selectors": ["input[data-testid=\"giftCodeInput\"]"], "type": "fill", "waitAfter": 5000, "required" : true, "event" : "[💳] Entering coupon code..."},
        { "name": "checkCoupon", "selectors": ["button[aria-label=\"Apply Gift Code\"]"], "type": "click", "waitAfter": 5000, "required" : true, "event" : "[✅] Checking coupon code..." }
      ],
      "waitTime": 3000,
      "codeValidation": {
        "element": "button[data-testid=\"changeGiftCode\"]",
        "validText": "Edit Code"
      },
      "clearCoupon": {
        "actions": [
          { "name": "openClearCoupon", "selectors": ["button[data-testid=\"changeGiftCode\"]"], "type": "click", "waitAfter": 3000, "required" : true, "event" : "[🛒] Open coupon code..." },
          { "name": "clearCoupon", "selectors": ["button[data-testid=\"cancelGiftCode\"]"], "type": "click", "waitAfter": 3000, "required" : true, "event" : "[🛒] Deleting coupon code..." }
        ]
      },
      "requestParams": {
        "applyCouponUrl":"https://api.modaoperandi.com/public/v3.5/cart",
        "method": "PATCH"
      }
    },
    "carturesti.ro": {
